<% if @data.length > 0 %>
  <% for link in @data["edges"] do %>
    <%= render partial: "link", locals: { link: link }%>
  <% end %>
<% else %>
  <div>No links yet!</div>
<% end %>
<div id="form"></div>
<div class="absolute bottom-0 left-0 right-0">
  <%= render partial: "next_prev_buttons", locals: { data: @data }%>
</div>
